{
  "name": "artibox",
  "description": "Artibox",
  "repository": "git@github.com:React-Artibox/artibox.git",
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean && lerna clean",
    "build:clean": "lerna run build:clean --stream --parallel",
    "build": "lerna run build --stream --sort",
    "format": "prettier './**/*.{js,ts,tsx,json,scss,md}' --write",
    "lint:css": "stylelint ./**/*.scss --config .stylelintrc",
    "lint:ts": "eslint . --config .eslintrc --ext .js,.ts,.tsx",
    "lint": "npm run lint:ts && npm run lint:css",
    "test:clean": "lerna run test:clean --stream --parallel",
    "test": "lerna run test --stream",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages --out ./public",
    "commit": "git-cz",
    "release": "npm run build:clean && lerna version && npm run build && lerna publish from-git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/cli": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@storybook/storybook-deployer": "^2.8.3",
    "@types/node": "^13.9.8",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9.7.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.1.0",
    "dart-sass": "^1.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lerna": "^3.20.2",
    "lint-staged": "^10.1.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "rollup": "^2.3.1",
    "rollup-plugin-postcss": "^2.5.0",
    "sass-loader": "^8.0.2",
    "slate": "^0.57.1",
    "slate-react": "^0.57.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  }
}
